{
  "skills": [
    {
      "name": "Scissors Feint",
      "description": "Enables players to execute the Scissors Feint at high speed."
    },
    {
      "name": "Double Touch",
      "description": "Enables players to quickly execute the Double Touch."
    },
    {
      "name": "Flip Flap",
      "description": "Enables players to execute the Flip Flap."
    },
    {
      "name": "Marseille Turn",
      "description": "Enables players to execute the Marseille Turn."
    },
    {
      "name": "Sombrero",
      "description": "Enables players to execute the Boomerang Trap and the Sombrero Turn."
    },
    {
      "name": "Chop Turn",
      "description": "Enables players to quickly execute the Chop Turn."
    },
    {
      "name": "Cut Behind & Turn",
      "description": "Enables players to execute the Cut Behind."
    },
    {
      "name": "Scotch Move",
      "description": "Enables players to execute the Scotch Move."
    },
    {
      "name": "Sole Control",
      "description": "Enables step on ball control to execute feints and turns."
    },
    {
      "name": "Heading",
      "description": "Improves the accuracy of headers as well as the frequency of downward headers."
    },
    {
      "name": "Long-Range Curler",
      "description": "Enables players to hit curling shots with great accuracy from a distance."
    },
    {
      "name": "Chip Shot Control",
      "description": "Enables accurate chip shots."
    },
    {
      "name": "Long-Range shooting",
      "description": "The player becomes more accurate at long-range shots."
    },
    {
      "name": "Knuckle Shot",
      "description": "Enables players to hit knuckle shots with greater ease, also applies to Free Kicks."
    },
    {
      "name": "Dipping Shot",
      "description": "Enables top spin shots that bounce erratically in front of the goalkeeper."
    },
    {
      "name": "Rising Shot",
      "description": "Enables shots that begin with a low trajectory that rise sharply."
    },
    {
      "name": "Acrobatic Finishing",
      "description": "Enables players to find a finish even from awkward positions or when off balance."
    },
    {
      "name": "Heel Trick",
      "description": "Enables players to pass and shoot using the heel, even from awkward positions or when off balance."
    },
    {
      "name": "First-time Shot",
      "description": "Improves technique and precision when taking first-time shots."
    },
    {
      "name": "One-touch Pass",
      "description": "Improves technique and precision when playing one-touch passes."
    },
    {
      "name": "Through Passing",
      "description": "Increases the accuracy of the player’s through balls."
    },
    {
      "name": "Weighted Pass",
      "description": "Enables players to apply backspin when playing lofted passes and through balls improving accuracy."
    },
    {
      "name": "Pinpoint Crossing",
      "description": "Enables players to curl in crosses with great accuracy."
    },
    {
      "name": "Outside Curler",
      "description": "Enables players to shoot and pass using the outside of the boot even at long distances."
    },
    {
      "name": "Rabona",
      "description": "Enables players to execute a Rabona and means they can pass or shoot when the opposition is not expecting it."
    },
    {
      "name": "No Look Pass",
      "description": "Enables passes that misdirect opponents."
    },
    {
      "name": "Low Lofted Pass",
      "description": "Enables players to hit a long accurate Lofted Pass with a low trajectory when appropriate."
    },
    {
      "name": "Long Throw",
      "description": "Improves the range of long throws."
    },
    {
      "name": "Penalty Specialist",
      "description": "Enables higher accuracy penalty kicks."
    },
    {
      "name": "Gamesmanship",
      "description": "Enables players to win free kicks when on the ball."
    },
    {
      "name": "Man Marking",
      "description": "Enables the player to stick to and persistently chase down an opponent."
    },
    {
      "name": "Track Back",
      "description": "The player aggressively pressures the opposition player who is on the ball all the way from the frontlines."
    },
    {
      "name": "Interception",
      "description": "Enables better interception skills."
    },
    {
      "name": "Blocker",
      "description": "Improves the player’s chances to block passes and shots."
    },
    {
      "name": "Aerial Superiority",
      "description": "Improves the player’s chances to win aerial balls."
    },
    {
      "name": "Sliding Tackle",
      "description": "Improves the player’s chances to gain ball control through a sliding tackle."
    },
    {
      "name": "Acrobatic Clearance",
      "description": "Enables players to clear the ball using their feet, even from awkward positions."
    },
    {
      "name": "Captaincy",
      "description": "Become the team’s inspiration on the pitch, reducing the effects of fatigue for all the team."
    },
    {
      "name": "Super-sub",
      "description": "Improves player’s abilities when introduced in the second half of the game."
    },
    {
      "name": "Fighting Spirit",
      "description": "Enables players to perform better under pressure and fatigue, as well as adversity."
    }
  ],
